{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Wedding</h1>

    <table class="record_properties">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ wedding.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ wedding.name }}</td>
        </tr>
        <tr>
            <th>Address</th>
            <td>{{ wedding.address }}</td>
        </tr>
        <tr>
            <th>Slug</th>
            <td>{{ wedding.slug }}</td>
        </tr>
        </tbody>
    </table>
{% endblock %}